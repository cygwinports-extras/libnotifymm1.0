  /usr/share/doc/libnotifymm-0.4.0/INSTALL
  /usr/share/doc/libnotifymm-0.4.0/README

(libnotifymm-devel)
  /usr/include/libnotifymm/libnotifymm.h
  /usr/include/libnotifymm/libnotifymm/init.h
  /usr/include/libnotifymm/libnotifymm/notification.h
  /usr/include/libnotifymm/libnotifymm/notify.h
  /usr/include/libnotifymm/libnotifymm/private/notification_p.h
  /usr/include/libnotifymm/libnotifymm/wrap_init.h
